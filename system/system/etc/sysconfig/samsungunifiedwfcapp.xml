<?xml version="1.0" encoding="UTF-8"?>
<config>
	<!-- These are the standard packages that are white-listed to always have internet access while in power save mode, even if they aren't in the foreground. -->
	<!-- unifiedwfc must run always for wifi calling servic. -->
	<allow-in-power-save package="com.sec.unifiedwfc" />
	<allow-in-power-save-except-idle package="com.sec.unifiedwfc" />
</config>